{
    "name": "colinjeanne/garden",
    "description": "Kathy's Garden Planner",
    "keywords": ["gardening", "garden"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "doctrine/orm": "~2.5",
        "google/apiclient": "2.0.*@dev",
        "respect/validation": "~2.0@dev",
        "php": "~5.6.0",
        "ext-openssl": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "compile": "npm run prepare"
    }
}
