{
    "name": "colinjeanne/garden",
    "description": "Kathy's Garden Planner",
    "keywords": ["gardening", "garden"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "doctrine/orm": "~2.5",
        "google/apiclient": "1.1.*",
        "respect/validation": "~2.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
